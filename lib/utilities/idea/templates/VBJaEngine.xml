<templateSet group="VBJaEngine">
  <template name="ass" value="ASSERT($VAR$, &quot;$CLS$::$FNC$: $MSG$&quot;);&#10;$END$" description="ASSERT check" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="&quot;this&quot;" alwaysStopAt="true" />
    <variable name="CLS" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <variable name="FNC" expression="" defaultValue="&quot;function&quot;" alwaysStopAt="true" />
    <variable name="MSG" expression="" defaultValue="&quot;null this&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="itv" value="for(node = VirtualList_begin($VLIST$); node; node = VirtualNode_getNext(node))&#10;{&#10;    $END$&#10;}&#10;" description="iterate VirtualList" toReformat="false" toShortenFQNames="true">
    <variable name="VLIST" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="nma" value="NM_ASSERT($VAR$, &quot;$CLS$::$FNC$: $MSG$&quot;);&#10;$END$" description="NM_ASSERT check" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="&quot;this&quot;" alwaysStopAt="true" />
    <variable name="CLS" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <variable name="FNC" expression="" defaultValue="&quot;function&quot;" alwaysStopAt="true" />
    <variable name="MSG" expression="" defaultValue="&quot;null this&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="geti" value="$NAME$_getInstance()$END$" description="get instance method" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;Class&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="itf19" value="ITOFIX19_13($VAL$)$END$" description="ITOFIX19_13" toReformat="false" toShortenFQNames="true">
    <variable name="VAL" expression="" defaultValue="&quot;Value&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="ftf19" value="FTOFIX19_13($VAL$)$END$" description="FTOFIX19_13" toReformat="false" toShortenFQNames="true">
    <variable name="VAL" expression="" defaultValue="&quot;Value&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="eed" value="extern EntityDefinition $NAME$;&#10;" description="extern EntityDefinition" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="capitalizeAndUnderscore(String)" defaultValue="&quot;ENTITY_NAME&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="ecd" value="extern CharSetDefinition $NAME$;&#10;" description="extern CharSetDefinition" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="capitalizeAndUnderscore(String)" defaultValue="&quot;CHARSET_NAME&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="div" value="&#10;&#10;//---------------------------------------------------------------------------------------------------------&#10;// &#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;$LABEL$&#10;//---------------------------------------------------------------------------------------------------------&#10;&#10;$END$" description="Optical divider" toReformat="false" toShortenFQNames="true">
    <variable name="LABEL" expression="enum(&quot;ASSETS&quot;, &quot;CLASS'S DEFINITION&quot;, &quot;CLASS'S METHODS&quot;, &quot;DECLARATIONS&quot;, &quot;DEFINITIONS&quot;, &quot;ENTRY POINTS&quot;, &quot;INCLUDES&quot;, &quot;MACROS&quot;, &quot;PROTOTYPES&quot;, &quot;STAGE DEFINITION&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="sc" value="__SAFE_CAST($CLS$, $VAR$)$END$" description="__SAFE_CAST" toReformat="false" toShortenFQNames="true">
    <variable name="CLS" expression="" defaultValue="&quot;Class&quot;" alwaysStopAt="true" />
    <variable name="VAR" expression="" defaultValue="&quot;Variable&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="unm" value="__UNIFORM_MOVEMENT" description="__UNIFORM_MOVEMENT" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="vec" value="VBVec3D $VAR$ = {&#10;    FTOFIX19_13($X$), &#10;    FTOFIX19_13($Y$), &#10;    FTOFIX19_13($Z$),&#10;};&#10;$END$" description="VBVec3D definition" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="snakeCase(String)" defaultValue="&quot;Variable&quot;" alwaysStopAt="true" />
    <variable name="X" expression="" defaultValue="0" alwaysStopAt="true" />
    <variable name="Y" expression="" defaultValue="0" alwaysStopAt="true" />
    <variable name="Z" expression="" defaultValue="0" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="vecc" value="VBVec3D $VAR$ = {FTOFIX19_13($X$), FTOFIX19_13($Y$), FTOFIX19_13($Z$)};&#10;$END$" description="VBVec3D definition (compact)" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="snakeCase(String)" defaultValue="&quot;Variable&quot;" alwaysStopAt="true" />
    <variable name="X" expression="" defaultValue="0" alwaysStopAt="true" />
    <variable name="Y" expression="" defaultValue="0" alwaysStopAt="true" />
    <variable name="Z" expression="" defaultValue="0" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="fun" value="// $CMT$&#10;$STATIC$$RET$ $CLS$_$FNC$($CLS$ this$ARG$)&#10;{&#10;&#9;ASSERT(this, &quot;$CLS$::$FNC$: null this&quot;);&#10;&#10;&#9;$END$&#10;}&#10;" description="function declaration" toReformat="true" toShortenFQNames="true">
    <variable name="CMT" expression="" defaultValue="&quot;description of function&quot;" alwaysStopAt="true" />
    <variable name="STATIC" expression="enum(&quot;&quot;, &quot;static &quot;, &quot;inline &quot;, &quot;inline static &quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="RET" expression="enum(&quot;void&quot;, &quot;int&quot;, &quot;bool&quot;, &quot;u8&quot;, &quot;u16&quot;, &quot;u32&quot;, &quot;s8&quot;, &quot;s16&quot;, &quot;s32&quot;, &quot;BYTE*&quot;, &quot;BgmapTexture&quot;, &quot;CharSet&quot;, &quot;Object&quot;, &quot;VBVec3D&quot;, &quot;VBVec3D*&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="CLS" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <variable name="FNC" expression="" defaultValue="&quot;functionName&quot;" alwaysStopAt="true" />
    <variable name="ARG" expression="" defaultValue="&quot;, Additional arguments&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="vcall" value="__VIRTUAL_CALL($ReturnType$, $ClassName$, $MethodName$, $Object$, $VariadicArgs$);&#10;$END$" description="__VIRTUAL_CALL" toReformat="false" toShortenFQNames="true">
    <variable name="ReturnType" expression="" defaultValue="&quot;ReturnType&quot;" alwaysStopAt="true" />
    <variable name="ClassName" expression="" defaultValue="&quot;ClassName&quot;" alwaysStopAt="true" />
    <variable name="MethodName" expression="" defaultValue="&quot;MethodName&quot;" alwaysStopAt="true" />
    <variable name="Object" expression="" defaultValue="&quot;Object&quot;" alwaysStopAt="true" />
    <variable name="VariadicArgs" expression="" defaultValue="&quot;VariadicArgs&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="gcl" value="__GET_CLASS_NAME($VAL$)$END$" description="__GET_CLASS_NAME" toReformat="false" toShortenFQNames="true">
    <variable name="VAL" expression="" defaultValue="&quot;Object&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="cfr" value="__CLASS_FRIEND_DEFINITION($VAL$)$END$" description="__CLASS_FRIEND_DEFINITION" toReformat="false" toShortenFQNames="true">
    <variable name="VAL" expression="" defaultValue="&quot;ClassName&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>